{
  "name": "cdk-watchful",
  "version": "0.3.0",
  "description": "Watching your CDK apps since 2019",
  "scripts": {
    "build": "jsii",
    "watch": "jsii -w",
    "package": "jsii-pacmak",
    "cdk": "cdk",
    "test": "jest"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "com.github.eladb.watchful",
        "maven": {
          "groupId": "com.github.eladb",
          "artifactId": "cdk-watchful"
        }
      },
      "python": {
        "distName": "cdk-watchful",
        "module": "cdk_watchful"
      }
    }
  },
  "author": {
    "email": "elad.benisrael@gmail.com",
    "name": "Elad Ben-Israel",
    "url": "https://github.com/eladb"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eladb/cdk-watchful"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-cdk/assert": "^1.0.0",
    "@types/jest": "^24.0.15",
    "@types/node": "12.6.2",
    "aws-cdk": "^1.0.0",
    "aws-sdk": "^2.492.0",
    "jest": "^24.8.0",
    "jsii": "^0.14.0",
    "jsii-pacmak": "^0.14.0",
    "typescript": "^3.5.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ]
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.0.0",
    "@aws-cdk/aws-cloudwatch": "^1.0.0",
    "@aws-cdk/aws-sns": "^1.0.0",
    "@aws-cdk/aws-sns-subscriptions": "^1.0.0",
    "@aws-cdk/aws-events": "^1.0.0",
    "@aws-cdk/aws-events-targets": "1.0.0",
    "@aws-cdk/aws-cloudwatch-actions": "1.0.0",
    "@aws-cdk/aws-dynamodb": "^1.0.0",
    "@aws-cdk/aws-sqs": "^1.0.0",
    "@aws-cdk/aws-lambda": "^1.0.0",
    "@aws-cdk/core": "^1.0.0"
  },
  "peerDependencies": {
    "@aws-cdk/aws-apigateway": "^1.0.0",
    "@aws-cdk/aws-sns": "^1.0.0",
    "@aws-cdk/aws-sns-subscriptions": "^1.0.0",
    "@aws-cdk/aws-cloudwatch": "^1.0.0",
    "@aws-cdk/aws-events": "^1.0.0",
    "@aws-cdk/aws-events-targets": "1.0.0",
    "@aws-cdk/aws-cloudwatch-actions": "1.0.0",
    "@aws-cdk/aws-dynamodb": "^1.0.0",
    "@aws-cdk/aws-sqs": "^1.0.0",
    "@aws-cdk/aws-lambda": "^1.0.0",
    "@aws-cdk/core": "^1.0.0"
  }
}
